
{
  "manifest_version": 2,

  "name": "안녕하세요 메모리레인 입니다.",
  "description": "Kimjinpyo",
  "version": "1.1",
  
  
  "background": {
      "page": "background.html",
    	  "persistent": true
   },
  
   "content_scripts":[{
	   "js": ["jquery-3.2.1.js" ],
   "matches": ["*://*/*"]
   
   }],
   
   
   
  "browser_action": {
    "default_icon": "Memory.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
	"\u003Call_urls>",
    "activeTab",
	"storage",
	"http://*/*",
	"https://*/*",
	"tabs"
  ]
  
  
   
}
